{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "camelcase": ["off"],
        "eqeqeq": ["off"],
        "linebreak-style": ["error", "windows"],
        "max-len": ["warn"],
        "no-plusplus":["off"]
    }
}
